<h2><%= @attraction.name %></h2>
<p>Ticket cost: <%= @attraction.tickets %></p>
<p>Min Height: <%= @attraction.min_height %>"</p>
<br>

<%= form_tag rides_path, method: :post do |form| %>
  <%= hidden_field_tag "ride[user_id]", current_user.id %>
  <%= hidden_field_tag "ride[attraction_id]", params[:id] %>
  <%= submit_tag "Go on this ride" %>
<% end %>